{
  "name": "www-ccorral",
  "version": "1.0.0",
  "description": "Carlos' website.",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "clear && webpack-dev-server",
    "clean": "rimraf build",
    "lint": "eslint app --fix || true",
    "test": "clear && jest ./tests --setupTestFrameworkScriptFile=raf/polyfill",
    "dev": "webpack-dashboard -t 'Webpack Dashboard' -- webpack-dev-server",
    "build": "npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "deploy": "npm run build && sh scripts/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccorral/www-ccorral.git"
  },
  "keywords": [
    "react",
    "webpack3",
    "boilerplate",
    "redux",
    "sass"
  ],
  "author": "Steve McKeogh",
  "bugs": {
    "url": "https://github.com/thekeogh/webpack-react-redux/issues"
  },
  "homepage": "https://github.com/thekeogh/webpack-react-redux/",
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.2.0",
